{
  "name": "kynsonszetau.com",
  "version": "2.0.0",
  "description": "The personal website of Kynson Szetau",
  "main": "src/main.ts",
  "scripts": {
    "build": "routify -b && snowpack build && rm -r public/.routify",
    "serve": "cp serve.json public/serve.json && serve public",
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:snowpack": "snowpack dev",
    "preinstall": "if [ -n '${CF_PAGES}' ]; then echo 'Renaming .npmrc-pages.txt to .npmrc'; mv .npmrc-pages.txt .npmrc; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kynson/kynsonszetau.com.git"
  },
  "author": " Kynson Szetau (https://kynsonszetau.com)",
  "bugs": {
    "url": "https://github.com/Kynson/kynsonszetau.com/issues"
  },
  "homepage": "https://github.com/Kynson/kynsonszetau.com#readme",
  "license": "MIT",
  "devDependencies": {
    "@roxi/routify": "^2.18.4",
    "@snowpack/plugin-optimize": "^0.2.13",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@snowpack/plugin-svelte": "^3.7.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/tailwindcss": "^3.0.5",
    "autoprefixer": "^10.4.2",
    "glob": "^7.1.7",
    "multistream": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "serve": "^13.0.2",
    "snowpack": "^3.8.8",
    "svelte": "^3.46.3",
    "tailwindcss": "^3.0.18",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@kynson/about": "^1.1.0"
  }
}